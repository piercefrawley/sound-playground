{
  "name": "html5-uploader",
  "version": "0.0.3",
  "description": "A pure HTML5 uploader",
  "author": {
    "name": "Michele Pangrazzi",
    "email": "xmikex83@gmail.com"
  },
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mpangrazzi/html5-uploader.git"
  },
  "scripts": {
    "test": "npm run build && cp ./dist/html5-uploader.js ./test/public && node ./test/server.js",
    "build": "browserify ./lib/index.js -s Uploader -o ./dist/html5-uploader.js",
    "build-min": "browserify ./lib/index.js -s Uploader | ./node_modules/.bin/uglifyjs -mc > ./dist/html5-uploader.min.js",
    "watch": "watchify ./examples/index.js ./lib/index.js -o ./examples/public/js/bundle.js -dv",
    "dev": "npm run watch & ./node_modules/.bin/supervisor ./examples/server.js"
  },
  "dependencies": {
    "eventemitter3": "^0.1.5"
  },
  "devDependencies": {
    "browserify": "^6.1.0",
    "express": "^4.9.8",
    "humanize": "0.0.9",
    "mocha": "^1.21.5",
    "should": "^4.0.4",
    "supervisor": "^0.6.0",
    "underscore": "^1.7.0",
    "multer": "^0.1.6",
    "uglify-js": "^2.4.15"
  },
  "keywords": [
    "html5",
    "uploader",
    "upload",
    "file",
    "drag",
    "drop"
  ],
